class Solution {
public:
    int findNumbers(vector<int>& nums) 
    {
        int count=0;
        int n;
        for(int i:nums)
        {
              string s=to_string(i);
              n=s.length();
              if(n % 2 == 0)
              {
                  count++;
              }
        }
        return count;
        
    }
};
